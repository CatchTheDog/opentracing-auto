    "eslint-plugin-promise": "4.2.1",
    "express": "4.17.1",
    "jaeger-client": "3.5.3",
    "knex": "0.20.10",
    "mocha": "7.0.1",
    "mongodb-core": "3.2.7",
    "monk": "7.1.2",
    "mysql": "2.15.0",
    "nock": "12.0.1",
    "npm-run-all": "4.1.1",
    "nyc": "15.0.0",
    "pg": "7.18.1",
    "redis": "3.0.2",
    "restify": "8.5.1",
    "sinon": "9.0.0",
    "sinon-chai": "3.5.0",
